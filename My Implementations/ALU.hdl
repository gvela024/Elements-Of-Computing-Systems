CHIP ALU
{
   IN in0[16], in1[16], zeroIn0, negateIn0, zeroIn1, negateIn1, function, negateOut0;
   OUT out0[16], zero, negative;

   PARTS:
      INV16(in0 = in0, out0 = in0Inverted);
      INV16(in0 = in1, out0 = in1Inverted);

      MUX4Way16(in0 = in0, in1 = in0Inverted, in2 = false, in3 = true, select[0] = zeroIn0, select[1] = negateIn0, out0 = operand0);
      MUX4Way16(in0 = in1, in1 = in1Inverted, in2 = false, in3 = true, select[0] = zeroIn1, select[1] = negateIn1, out0 = operand1);

      AND16B2-1(in0 = opreand0, in1 = operand1, output = andResult);
}
